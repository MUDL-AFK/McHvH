{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/main", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        }
      ]
    }
  ]
}